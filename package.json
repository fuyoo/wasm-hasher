  "name": "@fuyoo/wasm-hasher",
  "version": "0.1.15",
  "description": "computed data hash by webAssembly support md5,sha1,sha2,sha3,sm3",
  "keywords": [
    "md5",
    "sha",
    "sha1",
    "sha2",
    "sha3",
    "sha256",
    "sha384",
    "sha512",
    "wasm",
    "webassembly"
  ],
  "licenses": [
    "MIT"
  ],
  "main": "pkg/wasm_hasher.js",
  "scripts": {
    "build": "rimraf dist pkg && webpack",
